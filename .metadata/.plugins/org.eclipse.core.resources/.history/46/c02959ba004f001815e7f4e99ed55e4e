
public class Human {
	protected String = name;
	protected int strength;
	protected int intelligence;
	protected int stealth;
	protected int health;
	
	private void Human(String name) {
		this.name = name;
		this.intelligence = 3;
		this.strength = 3;
		this.stealth = 3;
		this.health = 100;
	}
	
	
}
