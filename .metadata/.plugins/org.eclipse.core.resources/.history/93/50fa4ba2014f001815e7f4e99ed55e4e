
public class Human {
	protected String name;
	protected int strength;
	protected int intelligence;
	protected int stealth;
	protected int health;
	
	private void Human(String name) {
		this.name = name;
		this.intelligence = 3;
		this.strength = 3;
		this.stealth = 3;
		this.health = 100;
	}
	
	protected void attack( Human target) {
		target.health -= this.strength;
	}
	
	public int void displayHealth(Human Target) {
		System.out.println(target.health);
	}
	
	
}
